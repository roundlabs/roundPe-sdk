{
  "version": "0.0.20",
  "license": "MIT",
  "main": "dist/roundpe",
  "typings": "dist/roundpe.d.ts",
  "repository":"https://github.com/roundlabs/roundPe-sdk/tree/main",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc && node dist/roundpe.js",
    "publish": "tsc && yarn publish"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "roundpe-sdk",
  "author": "Deepanshu Chahal",
  "module": "dist/roundpe-sdk.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/roundpe-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/roundpe-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "crypto": "^1.0.1"
  }
}
